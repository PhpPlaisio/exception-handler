{
  "name": "plaisio/exception-handler",
  "description": "PhpPlaisio: Exception Handler",
  "keywords": [
    "PhpPlaisio",
    "Exception",
    "Handler"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/response": "^2.2.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ExceptionHandler\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
